// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Coin{
  id Int @id @default(autoincrement())

  name String

  code String @unique

  rank Int

  image String

  price String

  marketCap String

  h24 String

}
